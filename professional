# aid 
alias aid='cd ~/../../dev/platform/aid/AnalyticsInsightsData'
alias scripts='web && cd Scripts'
alias aidjs='scripts'
alias aidweb='scripts'
alias aidcalc='cd ~/../../dev/platform/aid/AidCalc'
alias aiddatausage='cd ~/../../dev/platform/aid/AidDataUsage'
alias aiddu='aiddatausage'

# transaction attributes
alias ta='cd ~/../../dev/platform/aidtransactionattributescalc'
alias tacodes='cd ~/../../dev/platform/transactionattributecodes'

# data management
alias dm='cd ~/../../dev/platform/aiddatamanagement/AidDataManagement'
alias dmweb='dm && cd Apt.Platform.AidDataManagement.Web/Scripts'
alias dmjs='dmweb'

# mastermind
alias mm='web && cd mastermindScripts'
alias mmweb='mm'
alias mmjs='mm'

# navigation
alias documents='cd ~/../../Users/nmorrow/Documents'
alias downloads='cd ~/../../users/nmorrow/Downloads'
alias notes='documents && cd Notes'
alias projects='cd ~/../../users/nmorrow/Projects'
alias opennotes='code ~/../../Users/nmorrow/Projects/notes'
alias professional='projects && cd notes/professional'
alias openprofessional='code ~/../../Users/nmorrow/Projects/notes/professional'
alias dotfiles='cd ~/../../users/nmorrow/Projects/dotfiles'

# functions
function mkconfig() {
	sh ~/../../dev/platform/aid/Scripts/create-configurable-script.sh "$1" "$2"
}
